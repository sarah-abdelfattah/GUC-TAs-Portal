 res = await axiosCall(\"post\", \"departments/department\", body);\n\n      if (res.data.data) {\n        addToast(\"Department created successfully\", {\n          appearance: \"success\",\n          autoDismiss: true,\n        });\n        setFacultyChosen(\"\");\n        setHODChosen(\"\");\n        setName(\"\");\n      }\n\n      if (res.data.error) {\n        addToast(res.data.error, {\n          appearance: \"error\",\n          autoDismiss: true,\n        });\n      }\n    } catch (err) {\n      console.log(\"~err: \", err);\n    }\n  };\n\n  return (\n    <div className=\"crud-inner-container\">\n      <div className=\"crud-form\">\n        <FormControl className=\"crud-formControl\" required>\n          <InputLabel className=\"crud-inputLabel\">Faculty</InputLabel>\n          <Select\n            className=\"crud-select\"\n            value={facultyChosen}\n            onChange={(event) => {\n              setFacultyChosen(event.target.value);\n            }}\n          >\n            {faculties.length > 0 &&\n              faculties.map((faculty) => (\n                <MenuItem\n                  className=\"crud-menuItem\"\n                  value={faculty._id}\n                  key={faculty._id}\n                >\n                  {faculty.code} - {faculty.name}\n                </MenuItem>\n              ))}\n          </Select>\n          <FormHelperText className=\"crud-helperText\">\n            This field is required\n          </FormHelperText>\n        </FormControl>\n\n        <FormControl className=\"crud-formControl\" required>\n          <InputLabel className=\"crud-inputLabel\">Department Name</InputLabel>\n          <Input\n            className=\"crud-input\"\n            value={name}\n            onChange={(event) => setName(event.target.value)}\n          />\n          <FormHelperText className=\"crud-helperText\">\n            This field is required\n          </FormHelperText>\n        </FormControl>\n\n        <FormControl classNa