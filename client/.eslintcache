eam51/client/src/pages/AcademicMemberSchedule.jsx",["299"],"import React, { useState, useEffect } from \"react\";\r\nimport { useToasts } from \"react-toast-notifications\";\r\nimport Schedule from \"../components/Schedule\";\r\nimport checkLogin from \"../helpers/checkLogin\";\r\n\r\nfunction InstructorSlotsAssigned() {\r\n  const [gucId, setId] = useState(\"\");\r\n  const { addToast } = useToasts();\r\n\r\n  useEffect(() => {\r\n    async function fetchData() {\r\n      try {\r\n        const user = await checkLogin();\r\n        setId(user.gucId);\r\n      } catch (e) {\r\n        console.log(\"~ err\", e);\r\n        addToast(\"Sorry there is an error occurred, please try again later\", {\r\n          appearance: \"error\",\r\n          autoDismiss: true,\r\n        });\r\n      }\r\n    }\r\n    fetchData();\r\n  }, []);\r\n\r\n  return <div>{gucId ? <Schedule gucId={gucId} /> : null}</div>;\r\n}\r\n\r\nexport default InstructorSlotsAssigned;\r\n","/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/constants.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/checkLogin.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/axios.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/axiosCall.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/auth.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/helpers/setAuthToken.js",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/Update.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/Add.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/Delete.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/AttendanceRecord.jsx",[],["300","301"],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/sidebar/TAMenuItems.jsx",["302","303","304"],"import React, { useState } from \"react\";\nimport { Menu, MenuItem } from \"react-pro-sidebar\";\n\n//icons\nimport { MdLocationOn, MdPersonAdd } from \"react-icons/md\";\nimport { FaUniversity, FaClipboardList, FaBook } from \"react-icons/fa\";\n\nfunction TAMenuItems() {\n  const [icons, setIcons] = useState({\n    courseCoverage: false,\n    staff: false,\n  });\n\n  const routeChange = (path) => {\n    document.location.href = window.location.origin + `/${path}`;\n  };\n\n  const showTag = (name) => {\n    setIcons({\n      ...icons,\n      [name]: true,\n    });\n  };\n\n  return (\n    <Menu iconShape=\"round\" className=\"first-new\">\n      <MenuItem\n        icon={<MdPersonAdd />}\n        onMouseEnter={() => showTag(\"staff\")}\n        onMouseLeave={() => setIcons(false)}\n        onClick={() => routeChange(\"viewStaff\")}\n      >\n        {icons.staff ? \"Staff \" : \"\"}\n      </MenuItem>\n\n      <MenuItem\n        icon={<FaBook />}\n        onMouseEnter={() => showTag(\"course\")}\n        onMouseLeave={() => setIcons(false)}\n        onClick={() => routeChange(\"viewCourseCoverage\")}\n      >\n        {icons.courseCoverage ? \"Course Coverage\" : \"\"}\n      </MenuItem>\n    </Menu>\n  );\n}\n\nexport default TAMenuItems;\n","/mnt/d/My small Projects/milestone-1-team51/client/src/components/sidebar/CIMenuItems.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/sidebar/HODMenuItems.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/location/DeleteLocation.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/location/AddLocation.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/department/DeleteDepartment.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/faculty/DeleteFaculty.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/faculty/AddFaculty.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/client/src/components/faculty/UpdateFaculty.jsx",[],"/mnt/d/My small Projects/milestone-1-team51/c